{
  "name": "six-cities",
  "version": "4.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "npm run build && node ./dist/main.rest.js | npx pino-pretty --colorize --translateTime SYS:standard",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "serve:mock": "json-server ./mocks/mock-server-data.json --port 3000"
  },
  "devDependencies": {
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/express": "^5.0.5",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "dayjs": "^1.11.18",
    "eslint": "8.39.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "^0.17.4",
    "pino-pretty": "^13.1.2",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@typegoose/typegoose": "^11.5.0",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "got": "^14.4.9",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.1",
    "jose": "^6.1.3",
    "mime-types": "^3.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^7.5.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "pino": "^10.0.0",
    "reflect-metadata": "^0.2.2"
  }
}
